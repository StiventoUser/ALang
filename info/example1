function Main() -> Int32
{
	Int32 x, Int32 y = (1, 2);
	Int32 a, Int32 b;

	print x 
	print y

	(a, b) = (x, y) = (y*2, 3 - x / 2) + (1, 2 * y);

	print x
	print y

	print a
	print b
	
	A(x, y);
}
function A(Int32 a, Int32 b) -> Int32
{
	Int32 c, Int32 d = (a + b, a + b) * (1, -1);	
  
	print c
	print d
}
